{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "comma-dangle": "off",
        "object-curly-spacing": "off",
        "no-unused-vars": "warn",
        "new-cap": "off"
    }
}
